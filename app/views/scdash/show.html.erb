<!-- BEGIN PAGE CONTAINER-->
<div class="page-content">
  <div class="content">
    <!-- BEGIN PAGE TITLE -->
    <div class="page-title">
      <h3>Solutions Center Dashboard</h3>
    </div>
    <!-- END PAGE TITLE -->
    <!-- BEGIN PlACE PAGE CONTENT HERE -->
      <div id="container">
        <div class="row">
          <div class="col-md-3 col-vlg-3 col-sm-6">
            <div class="activeApplicant">
              <%= render partial: "active_applicant_tickets" %>
            </div>
          </div>
          <div class="col-md-3 col-vlg-3 col-sm-6">
            <div class="tiles red m-b-10">
              <div class="tiles-body">
                <%= content_tag(:div, link_to( content_tag(:i, 'refresh', class: "material-icons" ), "javascript:;", class: "reload" ), class: "controller") %>
                <div class="tiles-title"> TODAY'S SOLVED APPLICANT TICKETS </div>
                <div class="heading"> <span id="solvedAppTickets" class="animate-number" data-value="<%= @solvedAppTickets %>" data-animation-duration="1000">0</span></div>
              </div>
            </div>
          </div>
          <div class="col-md-3 col-vlg-3 col-sm-6">
            <div class="tiles green m-b-10">
              <div class="tiles-body">
                <%= content_tag(:div, link_to( content_tag(:i, 'refresh', class: "material-icons" ), "javascript:;", class: "reload" ), class: "controller") %>
                <div class="tiles-title"> TOTAL APPLICANT CHATS </div>
                <div class="heading"><span class="animate-number" data-value="<%= @totalAppChats %>" data-animation-duration="1000">0</span></div>
              </div>
            </div>
          </div>
          <div class="col-md-3 col-vlg-3 col-sm-6">
            <div class="tiles purple m-b-10">
              <div class="tiles-body">
                <%= content_tag(:div, link_to( content_tag(:i, 'refresh', class: "material-icons" ), "javascript:;", class: "reload" ), class: "controller") %>
                <div class="tiles-title"> TOTAL CHATS </div>
                <div class="row-fluid">
                  <div class="heading"> <span class="animate-number" data-value="<%= @totalChats %>" data-animation-duration="1200">0</span> </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div id="container">
        <div class="row">
          <div class="col-md-3 col-vlg-3 col-sm-6">
            <div class="tiles blue m-b-10">
              <div class="tiles-body">
                <%= content_tag(:div, link_to( content_tag(:i, 'refresh', class: "material-icons" ), "javascript:;", class: "reload" ), class: "controller") %>
                <div class="tiles-title"> ACTIVE RECOMMENDER TICKETS </div>
                <div class="heading"><span id="activeRecTickets" class="animate-number" data-value="<%= @activeRecTickets %>" data-animation-duration="700">0</span></div>
              </div>
            </div>
          </div>
          <div class="col-md-3 col-vlg-3 col-sm-6">
            <div class="tiles red m-b-10">
              <div class="tiles-body">
                <%= content_tag(:div, link_to( content_tag(:i, 'refresh', class: "material-icons" ), "javascript:;", class: "reload" ), class: "controller") %>
                <div class="tiles-title"> TODAY'S SOLVED RECOMMEDNER TICKETS </div>
                <div class="row-fluid">
                  <div class="heading"> <span id="totalRecTickets" class="animate-number" data-value="<%= @solvedRecTickets %>" data-animation-duration="1000">0</span> </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-md-3 col-vlg-3 col-sm-6">
            <div class="tiles green m-b-10">
              <div class="tiles-body">
                <%= content_tag(:div, link_to( content_tag(:i, 'refresh', class: "material-icons" ), "javascript:;", class: "reload" ), class: "controller") %>
                <div class="tiles-title"> TOTAL RECOMMENDER CHATS </div>
                <div class="heading"><span class="animate-number" data-value="<%= @totalRecChats %>" data-animation-duration="1000">0</span></div>
              </div>
            </div>
          </div>
          <div class="col-md-3 col-vlg-3 col-sm-6">
            <div class="tiles purple m-b-10">
              <div class="tiles-body">
                <%= content_tag(:div, link_to( content_tag(:i, 'refresh', class: "material-icons" ), "javascript:;", class: "reload" ), class: "controller") %>
                <div class="tiles-title"> TOTAL INTERACTIONS </div>
                <div class="row-fluid">
                  <div class="heading"> <span class="animate-number" data-value="<%= @totalInterations %>" data-animation-duration="1200">0</span> </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div id="container">
        <div class="row">
          <div class="col-md-4 col-vlg-4 col-sm-12">
            <div class="row">
              <div class="col-md-12">
                <div class="grid simple ">
                  <div class="grid-title no-border">
                    <h4>Tickets Answered <span class="semi-bold">Today</span></h4>
                    <div class="tools">
                      <%= link_to( content_tag(:i, 'more horiz', class: "material-icons" ), "javascript:;", class: "collapse" ) %>
                      <%= link_to( content_tag(:i, 'apps', class: "material-icons" ), "#grid-config", class: "config", 'data-toggle' => 'modal') %>
                      <%= link_to( content_tag(:i, 'refresh', class: "material-icons" ), "javascript:;", class: "reload" ) %>
                      <a href="javascript:;" class="remove"></a>
                    </div>
                  </div>
                  <div class="grid-body no-border">
                    <table class="table table-striped table-flip-scroll cf">
                      <thead class="cf">
                        <tr>
                          <th>Name</th>
                          <th>Tickets</th>
                          <th>Status</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% @ticketNameArray.each do |k, v| %>
                          <tr>
                            <td><%= k %></td>
                            <td><%= v %></td>
                            <td> <span class="label label-success">GOOD</span>
                            </td>
                          </tr>
                        <% end %>
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
</div>
<!-- END PAGE CONTAINER -->
