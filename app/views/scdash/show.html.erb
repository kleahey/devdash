<!-- BEGIN PAGE CONTAINER-->
<div class="page-content">
  <div class="content">
    <!-- BEGIN PAGE TITLE -->
    <div class="page-title">
      <h3>Solutions Center Dashboard</h3>
    </div>
    <!-- END PAGE TITLE -->
    <!-- BEGIN PlACE PAGE CONTENT HERE -->
      <div id="container">
        <div class="row 2col">
          <div class="col-md-3 col-sm-6 spacing-bottom-sm spacing-bottom">
            <div class="tiles blue added-margin">
              <div class="tiles-body">
                <%= content_tag(:div, link_to( content_tag(:i, 'refresh', class: "material-icons" ), "javascript:;", class: "reload" ), class: "controller") %>
                <div class="tiles-title"> ACTIVE APPLICANT TICKETS </div>
                <div class="heading"> <span class="animate-number" data-value="<%= @activeAppTickets %>" data-animation-duration="700">0</span></div>
              </div>
            </div>
          </div>
          <div class="col-md-3 col-sm-6 spacing-bottom-sm spacing-bottom">
            <div class="tiles red added-margin">
              <div class="tiles-body">
                <%= content_tag(:div, link_to( content_tag(:i, 'refresh', class: "material-icons" ), "javascript:;", class: "reload" ), class: "controller") %>
                <div class="tiles-title"> TODAY'S SOLVED APPLICANT TICKETS </div>
                <div class="heading"> <span class="animate-number" data-value="<%= @solvedAppTickets %>" data-animation-duration="1000">0</span></div>
              </div>
            </div>
          </div>
          <div class="col-md-3 col-sm-6 spacing-bottom">
            <div class="tiles green added-margin">
              <div class="tiles-body">
                <%= content_tag(:div, link_to( content_tag(:i, 'refresh', class: "material-icons" ), "javascript:;", class: "reload" ), class: "controller") %>
                <div class="tiles-title"> TOTAL APPLICANT CHATS </div>
                <div class="heading"><span class="animate-number" data-value="<%= @totalAppChats %>" data-animation-duration="1000">0</span></div>
              </div>
            </div>
          </div>
          <div class="col-md-3 col-sm-6">
            <div class="tiles purple added-margin">
              <div class="tiles-body">
                <%= content_tag(:div, link_to( content_tag(:i, 'refresh', class: "material-icons" ), "javascript:;", class: "reload" ), class: "controller") %>
                <div class="tiles-title"> TOTAL CHATS </div>
                <div class="row-fluid">
                  <div class="heading"> <span class="animate-number" data-value="<%= @totalChats %>" data-animation-duration="1200">0</span> </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="row 2col">
          <div class="col-md-3 col-sm-6 spacing-bottom">
            <div class="tiles blue added-margin">
              <div class="tiles-body">
                <%= content_tag(:div, link_to( content_tag(:i, 'refresh', class: "material-icons" ), "javascript:;", class: "reload" ), class: "controller") %>
                <div class="tiles-title"> ACTIVE RECOMMENDER TICKETS </div>
                <div class="heading"><span class="animate-number" data-value="<%= @activeRecTickets %>" data-animation-duration="700">0</span></div>
              </div>
            </div>
          </div>
          <div class="col-md-3 col-sm-6">
            <div class="tiles red added-margin">
              <div class="tiles-body">
                <%= content_tag(:div, link_to( content_tag(:i, 'refresh', class: "material-icons" ), "javascript:;", class: "reload" ), class: "controller") %>
                <div class="tiles-title"> TODAY'S SOLVED RECOMMEDNER TICKETS </div>
                <div class="row-fluid">
                  <div class="heading"> <span class="animate-number" data-value="<%= @solvedRecTickets %>" data-animation-duration="1000">0</span> </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-md-3 col-sm-6 spacing-bottom">
            <div class="tiles green added-margin">
              <div class="tiles-body">
                <%= content_tag(:div, link_to( content_tag(:i, 'refresh', class: "material-icons" ), "javascript:;", class: "reload" ), class: "controller") %>
                <div class="tiles-title"> TOTAL RECOMMENDER CHATS </div>
                <div class="heading"><span class="animate-number" data-value="<%= @totalRecChats %>" data-animation-duration="1000">0</span></div>
              </div>
            </div>
          </div>
          <div class="col-md-3 col-sm-6">
            <div class="tiles purple added-margin">
              <div class="tiles-body">
                <%= content_tag(:div, link_to( content_tag(:i, 'refresh', class: "material-icons" ), "javascript:;", class: "reload" ), class: "controller") %>
                <div class="tiles-title"> TOTAL INTERACTIONS </div>
                <div class="row-fluid">
                  <div class="heading"> <span class="animate-number" data-value="<%= @totalInterations %>" data-animation-duration="1200">0</span> </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="row 2col">
          <div class="col-md-4">
            <div class="row">
              <div class="col-md-12">
                <div class="grid simple ">
                  <div class="grid-title no-border">
                    <h4>Tickets Answered <span class="semi-bold">Yesterday</span></h4>
                    <div class="tools">
                      <a href="javascript:;" class="collapse"></a>
                      <a href="#grid-config" data-toggle="modal" class="config"></a>
                      <a href="javascript:;" class="reload"></a>
                      <a href="javascript:;" class="remove"></a>
                    </div>
                  </div>
                  <div class="grid-body no-border">
                    <table class="table table-striped table-flip-scroll cf">
                      <thead class="cf">
                        <tr>
                          <th>Name</th>
                          <th>Tickets</th>
                          <th>Status</th>
                        </tr>
                      </thead>
                      <tbody>
                        <tr>
                          <td>Mark</td>
                          <td>12</td>
                          <td> <span class="label label-important">ALERT!</span>
                          </td>
                        </tr>
                        <tr>
                          <td>
                            <div class="inline">Jacob </div>
                          </td>
                          <td>115</td>
                          <td><span class="label label-success">GOOD</span>
                          </td>
                        </tr>
                        <tr>
                          <td>Larry</td>
                          <td>92</td>
                          <td><span class="label label-success">GOOD</span>
                          </td>
                        </tr>
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="col-md-6">
        </div>
      </div>
    </div>
</div>
<!-- END PAGE CONTAINER -->
